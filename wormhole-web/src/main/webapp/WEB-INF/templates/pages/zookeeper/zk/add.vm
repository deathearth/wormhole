#set($layout = "/layout/layout.vm")
#set($pageTitle = "资源管理 - 新增")
#set($pageScript = "pages/zookeeper/zk/add")


<form class="form-horizontal">
  
    <div class="form-group">
        <label class="col-sm-2 col-sm-offset-1 control-label">ipport</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="ipport" placeholder="" value = "$!{ipport}" readonly="readonly">
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 col-sm-offset-1 control-label">root</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="root" placeholder="" value = "$!{root}" readonly="readonly">
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 col-sm-offset-1 control-label">名称</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="name" placeholder="" value = "">
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 col-sm-offset-1 control-label">值</label>
        <div class="col-sm-6">
            <input type="text" class="form-control" name="value" placeholder="" value = "">
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-6 col-sm-offset-3">
            <button type="button" class="btn btn-default" role="cancel">取消</button>
            <button type="submit" class="btn btn-primary">提交</button>
        </div>
    </div>

</form>